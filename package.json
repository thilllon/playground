{
  "name": "playground",
  "author": "thilllon",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "debug": "node --inspect-brk ./node_modules/next/dist/bin/next",
    "dev": "yarn clean && yarn && kill-port 3333 && next --port 3333",
    "pretty": "prettier --config .prettierrc.json --list-different --write \"./{components,pages,styles,utils}/**/*.{js,jsx,ts,tsx,graphql,gql}\"",
    "clean": "rimraf .next out node_modules/.cache && yarn cache clean && yarn pretty",
    "bs": "yarn build && yarn start",
    "predeploy": "yarn && yarn clean && cross-env SUBDIRECTORY=playground && yarn build && yarn export && touch out/.nojekyll",
    "deploy": "gh-pages --dist out --dotfiles false",
    "dp": "yarn deploy"
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^4.11.4",
    "@material-ui/data-grid": "^4.0.0-alpha.28",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@material-ui/styles": "^4.11.4",
    "axios": "^0.21.1",
    "clsx": "^1.1.1",
    "colors": "^1.4.0",
    "commander": "^7.2.0",
    "faker": "^5.5.3",
    "gray-matter": "^4.0.3",
    "next": "^10.2.0",
    "next-images": "^1.7.0",
    "next-optimized-images": "^2.6.2",
    "ora": "^5.4.0",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^17.0.2",
    "react-hotkeys-hook": "^3.3.1",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "babel-plugin-transform-define": "^2.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-remove-debugger": "^6.9.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-prettier": "^3.4.0",
    "gh-pages": "^3.1.0",
    "kill-port": "^1.6.1",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2"
  }
}
