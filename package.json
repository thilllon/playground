{
  "name": "gh-pages",
  "author": "thilllon",
  "license": "ISC",
  "version": "1.0.0",
  "scripts": {
    "debug": "node --inspect-brk ./node_modules/next/dist/bin/next",
    "dev": "yarn && kill-port 3333 && next --port 3333",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "deploy-to-github-pages": "yarn && yarn clean && next build && next export && touch out/.nojekyll && gh-pages --dist out --dotfiles false",
    "pretty": "prettier --list-different --ignore-unknown --write ./*.json \"./{src,__tests__,__mocks__}/**/*.{js,jsx,ts,tsx,graphql,gql}\"",
    "clean": "rimraf .next out node_modules/.cache && yarn cache clean",
    "bs": "yarn build && cross-env BACKEND_URL=  && yarn start",
    "dp": "yarn deploy-to-github-pages"
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.1.2",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@material-ui/core": "^4.11.3",
    "@material-ui/data-grid": "^4.0.0-alpha.27",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/styles": "^4.11.3",
    "axios": "^0.21.1",
    "babel-plugin-transform-remove-debugger": "^6.9.4",
    "clsx": "^1.1.1",
    "eslint-plugin-babel": "^5.3.1",
    "faker": "^5.3.1",
    "fs": "^0.0.1-security",
    "gray-matter": "^4.0.3",
    "next": "^10.0.6",
    "next-images": "^1.7.0",
    "next-optimized-images": "^2.6.2",
    "path": "^0.12.7",
    "react": "^17.0.1",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^17.0.1",
    "react-hotkeys-hook": "^3.3.1",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "babel-plugin-transform-define": "^2.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "gh-pages": "^3.1.0",
    "kill-port": "^1.6.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2"
  }
}
